#.SILENT:

prefix			= @prefix@
exec_prefix		= @exec_prefix@
cgidir			= @cgidir@

INSTALL			= @INSTALL@
RM			= rm -f

SRC_FILES		= nph-ftimes.cgi

DST_FILES		= ${cgidir}/nph-ftimes.cgi

all:

install: ${DST_FILES}
	umask 22; if [ ! -d ${cgidir} ]; then mkdir ${cgidir}; fi
	for i in ${SRC_FILES}; do ${INSTALL} -m 755 $$i ${cgidir}; done

clean:

clean-all: clean clean-auto

clean-auto:
	${RM} Makefile

dist-clean: clean-all

${DST_FILES}: ${SRC_FILES}
