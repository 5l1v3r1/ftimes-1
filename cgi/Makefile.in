#.SILENT:

prefix			= @prefix@
exec_prefix		= @exec_prefix@
cgidir			= @cgidir@
srcdir			= @srcdir@
VPATH			= ${srcdir}

INSTALL			= @INSTALL@
SRC_FILES		= nph-ftimes.cgi
DST_FILES		= ${cgidir}/nph-ftimes.cgi

all:

install: ${DST_FILES}
	umask 22; if [ ! -d ${cgidir} ]; then mkdir ${cgidir}; fi
	for i in ${SRC_FILES}; do\
		if [ -f ${srcdir}/$$i ]; then file=${srcdir}/$$i; else file=$$i; fi;\
		${INSTALL} -m 755 $$file ${cgidir};\
	done

clean:

clean-all: clean
	rm -f Makefile

${DST_FILES}: ${SRC_FILES}

